<div class="row">
  <div class="col-md-6">
    <h2>{{link-to 'Feeds' 'feeds'}} / {{model.onestop_id}}</h2>
  </div>
  <div class="col-md-6 text-right">
    {{feed-fetcher-controls feed=model}}
  </div>
</div>

{{#if model.latest_fetch_exception_log}}
  <div class="alert alert-danger">
    <strong>Latest fetch exception log</strong>: {{model.latest_fetch_exception_log}} (at {{model.last_fetched_at}})
  </div>
{{/if}}

{{feed-versions-list feed=model}}
