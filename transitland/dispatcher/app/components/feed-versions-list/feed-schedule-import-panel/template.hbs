{{#collapsable-panel title="Feed Schedule Import" panelClass=isSuccessful}}
  <dl class="dl-horizontal">
    <dt>Created at</dt>
    <dd>{{feedScheduleImport.created_at}}</dd>
    <dt>Updated at</dt>
    <dd>{{feedScheduleImport.updated_at}}</dd>
  </dl>
  {{#if feedScheduleImport.import_log}}
    {{#collapsable-panel title="Import Log"}}
      <pre>{{feedScheduleImport.import_log}}</pre>
    {{/collapsable-panel}}
  {{/if}}
  {{#if feedScheduleImport.exception_log}}
    {{#collapsable-panel title="Exception Log" panelClass="panel-danger"}}
      <pre>{{feedScheduleImport.exception_log}}</pre>
    {{/collapsable-panel}}
  {{/if}}
{{/collapsable-panel}}
